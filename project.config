[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/chaolinyi (chaolinyi)
[access "refs/for/refs/*"]
	read = group user/chaolinyi (chaolinyi)
	push = group user/chaolinyi (chaolinyi)
	label-Code-Review = -2..+2 group user/chaolinyi (chaolinyi)
	label-Verified = -1..+1 group user/chaolinyi (chaolinyi)
	rebase = group user/chaolinyi (chaolinyi)
	removeReviewer = group user/chaolinyi (chaolinyi)
	submit = group user/chaolinyi (chaolinyi)
[access "refs/heads/*"]
	read = group user/chaolinyi (chaolinyi)
	create = group user/chaolinyi (chaolinyi)
	push = +force group user/chaolinyi (chaolinyi)
	pushMerge = group user/chaolinyi (chaolinyi)
[access "refs/tags/*"]
	push = +force group user/chaolinyi (chaolinyi)
	pushTag = group user/chaolinyi (chaolinyi)
	pushSignedTag = group user/chaolinyi (chaolinyi)
